{
    "name": "napi-headers",
    "version": "0.5.0",
    "description": "Dowload Node.js N-API headers",
    "main": "./bin/index.js",
    "scripts": {
        "prepublishOnly": "pkg-ok",
        "test": "ava --verbose",
        "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
        "coverage": "nyc ava",
        "report": "nyc report --reporter=html"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fraxken/napi-headers.git"
    },
    "keywords": [
        "node",
        "nodejs",
        "n-api",
        "napi",
        "header",
        "cpp",
        "download"
    ],
    "author": "SlimIO",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/fraxken/napi-headers/issues"
    },
    "homepage": "https://github.com/fraxken/napi-headers#readme",
    "dependencies": {
        "@slimio/nodejs-downloader": "^0.4.0",
        "commander": "^2.20.0",
        "kleur": "^3.0.2",
        "libnpm": "^2.0.1",
        "make-promises-safe": "^4.0.0",
        "ora": "^3.1.0",
        "rmfr": "^2.0.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^7.5.2",
        "@commitlint/config-conventional": "^7.5.0",
        "@escommunity/minami": "^1.0.0",
        "@slimio/eslint-config": "^2.0.4",
        "@types/node": "^11.13.4",
        "ava": "^1.4.1",
        "cross-env": "^5.2.0",
        "eslint": "^5.14.0",
        "husky": "^1.3.1",
        "jsdoc": "^3.5.5",
        "nyc": "^13.3.0",
        "pkg-ok": "^2.3.1"
    },
    "engines": {
        "node": ">=10"
    },
    "husky": {
        "hooks": {
            "pre-push": "cross-env npm test",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "nyc": {
        "check-coverage": true,
        "per-file": false,
        "lines": 98,
        "statements": 98,
        "functions": 98,
        "branches": 90
    },
    "preferGlobal": true,
    "bin": {
        "napihead": "./bin/index.js",
        "nodehead": "./bin/index.js"
    }
}
